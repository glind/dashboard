# Environment variables for Personal Dashboard
# Copy this file to .env and replace placeholder values with real credentials

# Application Configuration
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO

# Todoist
TODOIST_API_TOKEN=your_todoist_token_here

# GitHub (Personal Access Token)
GITHUB_TOKEN=your_github_personal_access_token_here
GITHUB_USERNAME=your_github_username_here

# Buildly Labs
BUILDLY_BASE_URL=https://api.buildly.io
BUILDLY_API_KEY=your_buildly_api_key_here

# Weather API
OPENWEATHER_API_KEY=your_openweather_api_key_here

# Music Services
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here
YOUTUBE_API_KEY=your_youtube_api_key_here
LASTFM_API_KEY=your_lastfm_api_key_here

# News API
NEWS_API_KEY=your_news_api_key_here

# TickTick OAuth
TICKTICK_CLIENT_ID=your_ticktick_client_id_here
TICKTICK_CLIENT_SECRET=your_ticktick_client_secret_here

# Gmail OAuth (Google Cloud Console)
GMAIL_CLIENT_ID=your_gmail_client_id_here
GMAIL_CLIENT_SECRET=your_gmail_client_secret_here

# AI Providers
OPENAI_API_KEY=your_openai_api_key_here
