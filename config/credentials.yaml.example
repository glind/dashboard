# Credentials configuration
# Copy this to credentials.yaml and add your actual API keys

# Todoist API
todoist:
  api_token: "YOUR_TODOIST_API_TOKEN"

# GitHub API  
github:
  token: "YOUR_GITHUB_PERSONAL_ACCESS_TOKEN"
  username: "YOUR_GITHUB_USERNAME"

# Buildly Labs API
buildly:
  base_url: "https://api.buildly.io"
  api_key: "YOUR_BUILDLY_API_KEY"
