# Credentials configuration
# Copy this to credentials.yaml and add your actual API keys

# Google APIs (OAuth handled separately)
# Set up OAuth at: https://console.cloud.google.com
# Save OAuth config as: config/google_oauth_config.json
google:
  # OAuth credentials are handled via google_oauth_config.json
  # No manual tokens needed - use web authentication

# TickTick API
ticktick:
  # For client_id/secret OAuth flow:
  client_id: "YOUR_TICKTICK_CLIENT_ID"
  client_secret: "YOUR_TICKTICK_CLIENT_SECRET"
  # OR for direct token access:
  api_token: "YOUR_TICKTICK_API_TOKEN"

# Todoist API
todoist:
  api_token: "YOUR_TODOIST_API_TOKEN"

# GitHub API  
github:
  token: "YOUR_GITHUB_PERSONAL_ACCESS_TOKEN"
  username: "YOUR_GITHUB_USERNAME"

# Weather API
weather:
  api_key: "YOUR_OPENWEATHERMAP_API_KEY"

# News API (optional)
news:
  api_key: "YOUR_NEWS_API_KEY"

# Buildly Labs API
buildly:
  base_url: "https://api.buildly.io"
  api_key: "YOUR_BUILDLY_API_KEY"
